fn main() {
    let mut count = 0;
    let mut sum = 0;

    for i in 0..20 {
        if i % 2 == 0 {
            sum += i;
        } else {
            count += 1;
        }
    }

    print("Sum of evens:", sum);
    print("Count of odds:", count);

    let mut result = 0;
    for x in 0..15 {
        if x > 5 && x < 12 {
            result += x;
            if result > 30 {
                print("Result exceeded 30 at x:", x);
            }
        }
    }

    print("Final result:", result);

    let mut multiplied = 0;
    for j in 1..10 {
        if j % 3 == 0 {
            multiplied = j * 2;
            print("Multiple of 3:", j, "Doubled:", multiplied);
        }
    }
}
