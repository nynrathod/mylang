fn main() {
    let mut sum = 0;
    for i in 0..10 {
        sum += i;
        if sum > 20 {
            print("Sum exceeded 20 at i:", i);
        }
    }
    print("Final sum:", sum);

    let mut count = 0;
    for j in 0..5 {
        count += 1;
        print("Iteration:", j, "Count:", count);
    }
    print("Loop completed with count:", count);

    for k in 0..8 {
        if k == 4 {
            print("Reached middle iteration:", k);
        }
    }
}
