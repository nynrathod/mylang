fn increment(x: Int) -> Int {
    return x + 1;
}

fn double(x: Int) -> Int {
    return x * 2;
}

fn square(x: Int) -> Int {
    return x * x;
}

fn main() {
    let value = 5;
    print("Starting value:", value);

    let result1 = increment(value);
    print("After increment:", result1);

    let result2 = double(result1);
    print("After double:", result2);

    let result3 = square(result2);
    print("After square:", result3);

    let chained = square(double(increment(value)));
    print("Chained result:", chained);

    let x = 3;
    let y = increment(x);
    let z = double(y);
    let w = square(z);
    print("Step-by-step from 3:", w);

    for i in 0..5 {
        let transformed = square(double(increment(i)));
        print("Transform", i, ":", transformed);
    }
}
