fn createArray() -> [Int] {
    return [10, 20, 30];
}

fn getDoubledArray(arr: [Int]) -> [Int] {
    return [arr[0] * 2, arr[1] * 2, arr[2] * 2];
}

fn main() {
    let arr = createArray();
    print("Created array:");
    for i in 0..3 {
        print("arr[", i, "] =", arr[i]);
    }

    let doubled = getDoubledArray([5, 10, 15]);
    print("Doubled array:");
    for i in 0..3 {
        print("doubled[", i, "] =", doubled[i]);
    }
}
