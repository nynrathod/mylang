fn main() {
    let mut arr: [Int] = [1, 2, 3, 4, 5];
    print("Original array:");
    for i in 0..5 {
        print("arr[", i, "] =", arr[i]);
    }

    let mut total = 0;
    for i in 0..5 {
        total += arr[i];
    }
    print("Sum of all elements:", total);

    let mut count = 0;
    for i in 0..5 {
        if arr[i] > 2 {
            count += 1;
        }
    }
    print("Elements greater than 2:", count);

    let mut product = 1;
    for i in 0..5 {
        product = product * arr[i];
    }
    print("Product of elements:", product);
}
