fn main() {
    let a = true;
    let b = false;
    let c = true;

    print("Boolean AND:");
    let result1 = a && b;
    print("true && false =", result1);

    let result2 = a && c;
    print("true && true =", result2);

    print("Boolean OR:");
    let result3 = a || b;
    print("true || false =", result3);

    let result4 = b || b;
    print("false || false =", result4);

    print("Combined logic:");
    let result5 = a && b || c;
    print("true && false || true =", result5);

    let result6 = a || b && c;
    print("(true || false) && true =", result6);
}
