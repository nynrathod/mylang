fn main() {
    let mut total = 0;

    for i in 0..5 {
        if i == 0 {
            print("Starting loop");
        }

        for j in 0..3 {
            if j > 0 {
                total += i + j;
                if total > 10 {
                    print("Total exceeded 10, i =", i, ", j =", j);
                }
            }
        }

        if i == 4 {
            print("Loop ending");
        }
    }

    print("Final total:", total);

    let mut outerSum = 0;
    for a in 0..4 {
        let mut innerSum = 0;
        for b in 0..4 {
            if a > 0 && b > 0 {
                innerSum += a * b;
            }
        }
        outerSum += innerSum;
        if innerSum > 5 {
            print("Inner sum > 5 for a =", a, ", sum =", innerSum);
        }
    }
    print("Outer sum:", outerSum);
}
