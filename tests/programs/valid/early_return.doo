fn CheckValue(X: Int) -> Str {
    if X < 0 {
        return "Negative";
    }
    if X == 0 {
        return "Zero";
    }
    if X > 100 {
        return "Large";
    }
    return "Normal";
}

fn findFirstEven(Arr: [Int]) -> Int {
    for I in 0..5 {
        if Arr[I] % 2 == 0 {
            return Arr[I];
        }
    }
    return 0;
}

fn main() {
    let Result1 = CheckValue(-5);
    print("CheckValue(-5):", Result1);

    let Result2 = CheckValue(0);
    print("CheckValue(0):", Result2);

    let Result3 = CheckValue(50);
    print("CheckValue(50):", Result3);

    let Result4 = CheckValue(150);
    print("CheckValue(150):", Result4);

    let Arr: [Int] = [1, 2, 3, 4, 5];
    let Found = findFirstEven(Arr);
    print("First even found:", Found);
}
