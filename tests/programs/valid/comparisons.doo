fn main() {
    let a = 10;
    let b = 20;
    let c = 10;

    print("Equality:");
    let eq1 = a == b;
    print("10 == 20:", eq1);

    let eq2 = a == c;
    print("10 == 10:", eq2);

    print("Inequality:");
    let neq1 = a != b;
    print("10 != 20:", neq1);

    let neq2 = a != c;
    print("10 != 10:", neq2);

    print("Less than:");
    let lt1 = a < b;
    print("10 < 20:", lt1);

    let lt2 = b < a;
    print("20 < 10:", lt2);

    print("Greater than:");
    let gt1 = a > b;
    print("10 > 20:", gt1);

    let gt2 = b > a;
    print("20 > 10:", gt2);

    print("Less than or equal:");
    let le1 = a <= c;
    print("10 <= 10:", le1);

    let le2 = a <= b;
    print("10 <= 20:", le2);

    print("Greater than or equal:");
    let ge1 = a >= c;
    print("10 >= 10:", ge1);

    let ge2 = b >= a;
    print("20 >= 10:", ge2);
}
