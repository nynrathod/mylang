fn add(x: Int, y: Int) -> Int {
    return x + y;
}

fn multiply(x: Int, y: Int) -> Int {
    return x * y;
}

fn subtract(x: Int, y: Int) -> Int {
    return x - y;
}

fn main() {
    let result1 = add(multiply(2, 3), 4);
    print("add(multiply(2, 3), 4) =", result1);

    let result2 = multiply(add(5, 3), subtract(10, 2));
    print("multiply(add(5, 3), subtract(10, 2)) =", result2);

    let result3 = subtract(multiply(10, 2), add(5, 5));
    print("subtract(multiply(10, 2), add(5, 5)) =", result3);

    let a = add(1, 2);
    let b = multiply(a, 3);
    let c = subtract(b, 4);
    print("Chained result:", c);
}
