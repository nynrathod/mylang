fn sumArray(arr: [Int]) -> Int {
    let mut total = 0;
    for i in 0..5 {
        total += arr[i];
    }
    return total;
}

fn countGreater(arr: [Int], threshold: Int) -> Int {
    let mut count = 0;
    for i in 0..5 {
        if arr[i] > threshold {
            count += 1;
        }
    }
    return count;
}

fn main() {
    let numbers: [Int] = [5, 15, 10, 25, 8];

    let sum = sumArray(numbers);
    print("Sum of array:", sum);

    let aboveTen = countGreater(numbers, 10);
    print("Elements above 10:", aboveTen);

    let aboveFive = countGreater(numbers, 5);
    print("Elements above 5:", aboveFive);
}
