fn main() {
    let mut count = 0;
    for i in 0..100 {
        count += 1;
    }
    print("Loop count 100:", count);

    let mut sum = 0;
    for i in 0..50 {
        sum += i;
    }
    print("Sum 0 to 50:", sum);

    let mut nestedTotal = 0;
    for i in 0..10 {
        for j in 0..10 {
            nestedTotal += 1;
        }
    }
    print("Nested loop total:", nestedTotal);
}
