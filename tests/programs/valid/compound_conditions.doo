fn main() {
    let x = 10;
    let y = 20;
    let z = 15;

    print("Compound conditions test:");

    if x < y && y > z {
        print("x < y AND y > z: true");
    }

    if x < y || z > y {
        print("x < y OR z > y: true");
    }

    if x < y && y > z && z > x {
        print("All three conditions true");
    }

    if x == 10 && y == 20 && z == 15 {
        print("Exact equality check passed");
    }

    if x != y && y != z && x != z {
        print("All values are different");
    }

    let mut count = 0;
    for i in 0..20 {
        if i > 5 && i < 15 {
            count += 1;
        }
    }
    print("Count of i where 5 < i < 15:", count);

    if x <= 10 && y >= 20 {
        print("x <= 10 and y >= 20: true");
    }
}
