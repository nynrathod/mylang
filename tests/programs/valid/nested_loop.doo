fn main() {
    let mut total = 0;

    for i in 0..3 {
        for j in 0..3 {
            if i > 0 && j > 0 {
                total += 1;
                print("i =", i, ", j =", j);
            }
        }
    }

    print("Total iterations with condition:", total);

    let mut counter = 0;
    for x in 0..5 {
        for y in 0..4 {
            counter += 1;
        }
    }
    print("Total iterations nested:", counter);
}
