fn main() {
    let x = 15;

    if x > 20 {
        print("x is greater than 20");
    } else {
        print("x is not greater than 20");
    }

    let y = 30;
    if y > 20 {
        print("y is greater than 20");
    }

    let z = 5;
    if z > 10 {
        print("z is greater than 10");
    } else if z > 0 {
        print("z is positive but not greater than 10");
    } else {
        print("z is not positive");
    }

    let mut count = 0;
    for i in 0..10 {
        if i > 5 {
            count += 1;
        }
    }
    print("Count of i > 5:", count);
}
