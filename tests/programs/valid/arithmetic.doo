fn main() {
    let a = 10;
    let b = 5;
    let c = 2;

    print("Basic arithmetic:");
    let addResult = a + b;
    print("10 + 5 =", addResult);

    let subResult = a - b;
    print("10 - 5 =", subResult);

    let mulresult = a * b;
    print("10 * 5 =", mulresult);

    let divResult = a / b;
    print("10 / 5 =", divResult);

    print("Operator precedence:");
    let prec1 = a + b * c;
    print("10 + 5 * 2 =", prec1);

    let prec3 = a * b + c;
    print("10 * 5 + 2 =", prec3);

    let prec4 = a / b + c;
    print("10 / 5 + 2 =", prec4);

    let mut result = 100;
    result = result - 25;
    result = result + 10;
    result = result * 2;
    print("Final result after operations:", result);
}
