fn main() {
    // Arrays
    let numbers: [Int] = [1, 2, 3, 4, 5];
    print("Numbers:", numbers);

    for n in numbers {
        print("Number:", n);
    }

    // Maps
    let scores: {Str: Int} = {"Alice": 95, "Bob": 87, "Charlie": 92};
    print("Scores:", scores);

    for (name, score) in scores {
        print(name, "scored", score);
    }
}
