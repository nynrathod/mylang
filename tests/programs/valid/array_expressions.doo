fn main() {
    let x = 5;
    let y = 10;

    let arr: [Int] = [x + y, x - y, x * y, x / y];
    print("Array with expressions:");
    for i in 0..4 {
        let elem = arr[i];
        print("arr[", i, "] =", elem);
    }

    let arr2: [Int] = [1 + 2, 3 * 4, 5 - 1, 10 / 2];
    print("Direct expression array:");
    for i in 0..4 {
        print("arr2[", i, "] =", arr2[i]);
    }

    let base = 100;
    let arr3: [Int] = [base, base + 10, base + 20, base + 30];
    print("Array with base reference:");
    for i in 0..4 {
        print("arr3[", i, "] =", arr3[i]);
    }
}
