fn greet(firstname: Str, lastname: Str) -> Str {
    return firstname + lastname;
}

fn multiply(a: Int, b: Int, c: Int) -> Int {
    return a * b * c;
}

fn main() {
    let name = greet("John", "Doe");
    print("Name:", name);

    let product = multiply(2, 3, 4);
    print("Product:", product);

    let result = greet("Hello", "World");
    print(result);
}
