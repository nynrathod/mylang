import core::validation::IsValidAge;
import core::validation::IsValidScore;

fn And(a: Int, b: Int) -> Int {
    if a == 1 {
        if b == 1 {
            return 1;
        }
    }
    return 0;
}

fn Or(a: Int, b: Int) -> Int {
    if a == 1 {
        return 1;
    }
    if b == 1 {
        return 1;
    }
    return 0;
}

fn Not(x: Int) -> Int {
    if x == 1 {
        return 0;
    }
    return 1;
}

fn ValidateAgeAndScore(age: Int, score: Int) -> Int {
    let ageOk = IsValidAge(age);
    let scoreOk = IsValidScore(score);
    return And(ageOk, scoreOk);
}
