fn Add(a: Int, b: Int) -> Int {
    return a + b;
}

fn Subtract(a: Int, b: Int) -> Int {
    return a - b;
}

fn Multiply(a: Int, b: Int) -> Int {
    return a * b;
}

fn Divide(a: Int, b: Int) -> Int {
    if b == 0 {
        return 0;
    }
    return a / b;
}

fn Square(x: Int) -> Int {
    return Multiply(x, x);
}

fn Abs(x: Int) -> Int {
    if x < 0 {
        return Subtract(0, x);
    }
    return x;
}
